      <input\n            type=\"password\"\n            id=\"new-password\"\n            minLength=\"7\"\n            ref={newPassRef}\n          />\n          <label htmlFor=\"new-password-confirm\">Confirm New Password</label>\n          <input\n            type=\"password\"\n            id=\"new-password-confirm\"\n            minLength=\"7\"\n            ref={newPassConfRef}\n          />\n        </div>\n        <div className={classes.action}>\n          {isLoading ? (\n            \"Working on it...\"\n          ) : (\n            <button onClick={changePasswordHandler}>Change Password</button>\n          )}\n        </div>\n      </form>\n    </>\n  );\n};\n\nexport default ProfileForm;\n","/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/store/store.js",[],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/store/sagas.js",["75"],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/store/reducers.js",["76"],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/store/actions.js",[],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/components/StartingPage/PostItemPageContent.js",["77"],{"ruleId":"78","replacedBy":"79"},{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","severity":1,"message":"83","line":73,"column":6,"nodeType":"84","endLine":73,"endColumn":8,"suggestions":"85"},{"ruleId":"86","severity":1,"message":"87","line":2,"column":21,"nodeType":"88","messageId":"89","endLine":2,"endColumn":29},{"ruleId":"86","severity":1,"message":"90","line":21,"column":10,"nodeType":"88","messageId":"89","endLine":21,"endColumn":26},{"ruleId":"86","severity":1,"message":"91","line":24,"column":10,"nodeType":"88","messageId":"89","endLine":24,"endColumn":24},{"ruleId":"86","severity":1,"message":"92","line":24,"column":26,"nodeType":"88","messageId":"89","endLine":24,"endColumn":43},{"ruleId":"82","severity":1,"message":"93","line":32,"column":6,"nodeType":"84","endLine":32