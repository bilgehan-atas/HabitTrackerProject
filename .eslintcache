te\n          path=\"/profile\"\n          element={\n            isLoggedIn ? (\n              <UserProfile />\n            ) : (\n              <Navigate replace={true} to={\"/auth\"} />\n            )\n          }\n        />\n        <Route path=\"*\" element={<Navigate replace={true} to={\"/\"} />} />\n      </Route>\n    )\n  );\n\n  // get user data from cookie\n\n  const dispatch = useDispatch();\n\n  const checkCookie = () => {\n    let dc = document.cookie;\n    let prefix = \"token=\";\n    let begin = dc.indexOf(\"; \" + prefix);\n    if (begin === -1) {\n      begin = dc.indexOf(prefix);\n      if (begin !== 0) return null;\n    } else {\n      begin += 2;\n      var end = document.cookie.indexOf(\";\", begin);\n      if (end === -1) {\n        end = dc.length;\n      }\n    }\n    return decodeURI(dc.substring(begin + prefix.length, end));\n  };\n\n  const manageCookie = () => {\n    let token = checkCookie();\n    if (token !== null && token !== undefined && token !== \"undefined\") {\n      dispatch(getUserData(token));\n    }\n  };\n\n  useEffect(() => {\n    manageCookie();\n  }, []);\n\n  return <RouterProvider router={router} />;\n};\n\nexport default App;\n","/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/pages/HomePage.js",["68"],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/pages/AuthPage.js",[],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/components/Layout/Layout.js",[],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/components/Profile/UserProfile.js",[],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/components/Layout/MainNavigation.js",[],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/components/StartingPage/StartingPageContent.js",["69","70","71","72","73"],"/Users/bilgehanatas/Documents/Code/React/Redux-Saga-Dummy-Project/src/components/Auth/AuthForm.js",[],"/Users/bilgehanatas/Documents/Code/Reac